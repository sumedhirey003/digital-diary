{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/entries",
      "dest": "server/api/entries.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/app.js"
    }
  ]
}
